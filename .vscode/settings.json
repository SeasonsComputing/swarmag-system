{
  "cSpell.customDictionaries": {
    "custom-dictionary-workspace": {
      "name": "custom-dictionary-workspace",
      "path": "${workspaceFolder:swarmag-system}/.cspell/custom-dictionary-workspace.txt",
      "addWords": true,
      "scope": "workspace"
    }
  },
  "editor.formatOnSave": true,
  "dprint.path": "/home/tedvkremer/.deno/bin/dprint",
  "editor.defaultFormatter": "dprint.dprint",
  "[typescript]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[json]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[markdown]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "deno.enable": true,
  "deno.lint": true
}
